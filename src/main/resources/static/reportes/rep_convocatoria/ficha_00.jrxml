	<queryString>
		<![CDATA[select       (A.ape_paterno||' '||A.ape_materno||' '||A.nombres) as appnomb_aspirante,
               (select trim(nomb_dpto_dpt) from tdepartamentos where A.cod_dep_nac = tdepartamentos.codi_depa_dpt) ||' - '|| (select trim(nomb_prov_tpr) from tprovincias    where A.cod_dep_nac = tprovincias.codi_depa_dpt and A.cod_pro_nac = codi_prov_tpr) ||' - '||(select trim(nomb_dist_tdi) from tdistritos  where A.cod_dep_nac = tdistritos.codi_depa_dpt and A.cod_pro_nac = tdistritos.codi_prov_tpr and A.cod_dis_nac = codi_dist_tdi) as LUGAR_NACIMIENTO,
               to_char(A.fecha_nac,'dd/mm/yyyy') as sfecha_nacimiento,
               HF.domicilio || ' - ' ||
               (select trim(nomb_dpto_dpt) from tdepartamentos where HF.cod_dep_domi = tdepartamentos.codi_depa_dpt) || ' - ' ||
               (select trim(nomb_prov_tpr) from tprovincias    where HF.cod_dep_domi = tprovincias.codi_depa_dpt and HF.cod_pro_domi = codi_prov_tpr) || ' - ' ||
               (select trim(nomb_dist_tdi) from tdistritos     where HF.cod_dep_domi = tdistritos.codi_depa_dpt and HF.cod_pro_domi = tdistritos.codi_prov_tpr and HF.cod_dis_domi = codi_dist_tdi) as domicilio,
               decode(A.tipo_doc,'01','DNI: '||A.num_documento  ,'Carnet extranjería: ' ||A.num_documento) as tipo_doc,

               decode(HF.estado_civil,'S',decode(A.genero,'F','Soltera','Soltero'), decode(A.genero,'F','Casada','Casado')) as estado_civil,
               decode(A.genero,'F','Femenino','Masculino') as genero,
               HF.tel_cell,
               HF.tel_fijo,
               HF.email,
               I.id_inscripcion,
               C.id_convocatoria,
               C.nro_convocatoria,
               C.anio,
               A.id_aspirante,
	     A.ind_discapacidad,
               A.tipo_discapacidad,
               I.activo,
               I.necesita_ajuste_discapacidad,
	     I.detalle_ajuste_discapacidad,
               I.redam,
               I.rnsdd,
               I.antecedentes_penales,
	     I.antecedentes_policiales,
 	     I.antecedentes_judiciales,
	     I.ind_sentencia_delito,
	     I.ind_impedimento_juridico,
               I.ind_grado_parentesco,
               I.detalle_grado_parentesco,
               decode(I.estado_inscripcion,'N','Nuevo','S','Seleccionado','R','Rechazado','G','Ganador') as estado_inscripcion,
               A.activo,
               TO_CHAR(fecha_egreso,'DD-MM-YYYY') as sfecha_egreso,
               HF.centro_superior,hf.competencias_per,
REPLACE(hf.conocimientos,chr(13)||chr(10),',') as conocimientos,
REPLACE(hf.conocimiento_ofimatica,chr(13)||chr(10),',')  as conocimiento_ofimatica,hf.conocimiento_idiomas,
	    decode(hf.promedio_academico,'00','Regular','03','Tercio Superior','05','Quinto Superior','10','Decimo Superior')
	    as promedio_academico,
               DECODE(hf.tipo_formacion,'U','Universitario','T', 'Tecnico') as tipo_formacion,
               E.nombre_especialidad,
               n.nombre_nivel_alcanzado,
	( select count(*) from  prac_hist_experiencia HE
	where
          HE.id_inscripcion     =   to_number($P{id_inscripcion})
	and HE.nombre_institucion='DEFENSORÍA DEL PUEBLO') as nro_practicas_dp,
	( select count(*) from  prac_hist_experiencia HE
	where
          HE.id_inscripcion     =   to_number($P{id_inscripcion})
	and HE.nombre_institucion!='DEFENSORÍA DEL PUEBLO') as nro_practicas_otros

          from prac_convocatoria C, prac_inscripcion I,
	prac_aspirante A, prac_hist_formacion HF, prac_especialidad E,prac_nivel_alcanzado N
        where     I.id_inscripcion          =  to_number($P{id_inscripcion}) and
                  C.id_convocatoria         =  I.id_convocatoria   and
                  I.id_aspirante            =  A.id_aspirante and
                  HF.id_inscripcion         =  I.id_inscripcion and
                  HF.id_especiallidad       =  E.id_especialidad and
                  hf.id_nivel_alcanzado     = N.id_nivel_alcanzado]]>
	</queryString>
